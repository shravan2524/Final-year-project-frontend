const sortFn = (a, b) => {
  if (a.columnTitle === b.columnTitle) {
    if (a.columnName < b.columnName) {
      return -1;
    }
    return 1;
  }
  if (a.columnTitle < b.columnTitle) {
    return -1;
  }
  return 1;
};


export const columnTypes = [
  {
    id: 'string',
